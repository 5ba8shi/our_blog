<div class="content-log">
  <h2>ログイン</h2>

  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="field">
      <%= f.label :メールアドレス %><br />
      <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
    </div>

    <div class="field">
      <%= f.label :パスワード %><br />
      <%= f.password_field :password, autocomplete: "current-password" %>
    </div>

    <% if devise_mapping.rememberable? %>
      <div class="field">
        <%= f.check_box :remember_me %>
        <%= f.label :パスワードを記憶させる %>
      </div>
    <% end %>

    <div class="actions">
      <%= f.submit "ログイン" %>
    </div>
  <% end %>
  <ul class= "sns_register">
    <li class= "sns_facebook"><%= link_to "Facebookでログイン", user_facebook_omniauth_authorize_path, method: :post %></li><br>
    <li class= "sns_google"><%= link_to "Googleでログイン", user_google_oauth2_omniauth_authorize_path, method: :post %></li>
  </ul>
</div>